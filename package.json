{
    "name": "vue-antd-admin",
    "version": "0.7.4",
    "homepage": "https://iczer.github.io/vue-antd-admin",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:dev": "vue-cli-service build --mode development",
        "build:qa": "vue-cli-service build --mode qa",
        "build:rc": "vue-cli-service build --mode rc",
        "build:prod": "vue-cli-service build --mode prod",
        "lint": "vue-cli-service lint",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d dist -b pages -r https://gitee.com/iczer/vue-antd-admin.git",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "docs:deploy": "vuepress build docs && gh-pages -d docs/.vuepress/dist -b master -r https://gitee.com/iczer/vue-antd-admin-docs.git"
    },
    "dependencies": {
        "@antv/data-set": "^0.11.4",
        "@tinymce/tinymce-vue": "^5.0.0",
        "@vant/area-data": "^1.3.1",
        "@wangeditor/editor": "^5.1.12",
        "@wangeditor/editor-for-vue": "^1.0.2",
        "ali-oss": "^6.17.1",
        "animate.css": "^4.1.0",
        "ant-design-vue": "1.7.2",
        "axios": "^0.19.2",
        "blueimp-md5": "^2.19.0",
        "clipboard": "^2.0.6",
        "core-js": "^3.6.5",
        "date-fns": "^2.14.0",
        "dayjs": "^1.11.4",
        "echarts": "^5.3.3",
        "enquire.js": "^2.1.6",
        "highlight.js": "^10.2.1",
        "js-cookie": "^2.2.1",
        "mockjs": "^1.1.0",
        "nprogress": "^0.2.0",
        "tinymce": "^6.1.0",
        "vue": "^2.7.8",
        "vue-i18n": "^8.18.2",
        "vue-router": "^3.3.4",
        "vuedraggable": "^2.23.2",
        "vuex": "^3.4.0"
    },
    "devDependencies": {
        "@ant-design/colors": "^4.0.1",
        "@babel/eslint-parser": "^7.18.9",
        "@vue/cli-plugin-babel": "^4.4.0",
        "@vue/cli-plugin-eslint": "^4.4.0",
        "@vue/cli-service": "^4.4.0",
        "@vuepress/plugin-back-to-top": "^1.5.2",
        "babel-eslint": "^10.1.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "babel-polyfill": "^6.26.0",
        "compression-webpack-plugin": "^2.0.0",
        "cz-git": "^1.3.11",
        "deepmerge": "^4.2.2",
        "eslint": "^8.18.0",
        "eslint-plugin-vue": "^9.1.1",
        "fast-deep-equal": "^3.1.3",
        "gh-pages": "^3.1.0",
        "less-loader": "^6.1.1",
        "style-resources-loader": "^1.3.2",
        "vue-cli-plugin-style-resources-loader": "^0.1.4",
        "vuepress": "^1.5.2",
        "webpack-theme-color-replacer": "1.3.18",
        "whatwg-fetch": "^3.0.0"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git",
            "useEmoji": true
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ]
}
